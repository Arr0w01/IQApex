<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.IQApex" parent="Theme.MaterialComponents.Light">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/active</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">#C7C6C6</item>

        <!-- Status bar color. -->
        <!-- Customize your theme here. -->

        <item name="android:statusBarColor" tools:targetApi="l">@color/theme</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowLightStatusBar" tools:ignore="NewApi">true</item>
    </style>
    <style name="navColor" parent="Theme.AppCompat">
        <item name="colorAccent">@color/white</item>
        <item name="android:textColorSecondary">@color/active</item>
    </style>

</resources>